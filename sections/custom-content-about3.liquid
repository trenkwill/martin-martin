<div>
  {% if section.settings.title != blank %}
    <div class="section-header text-center">
      <h4 class="h2">{{ section.settings.title | escape }}</h4>
    </div>
  {% endif %}


  <div class="custom-content custom-content-about2">
    {%- assign block_width_mobile = '' -%}

      {% for block in section.blocks %}
      <div class="medium--one-third custom__item custom__item--{{ block.type }} custom__item--{{ block.id }} {% if block.settings.alignment %} {% endif %}" {{ block.shopify_attributes }}>
        <div class="custom__item-inner custom__item-inner--{{ block.type }}"
        {% if block.type == 'image' %} id="{{ img_wrapper_id }}" style="text-align: right;"{% endif %}>
          {% case block.type %}
            {% when 'image' %}
              {% if block.settings.image != blank %}
                 {% capture image_size %}{% endcapture %}
                  {{ block.settings.image | img_url: image_size, scale: 2, crop: 'top' | img_tag: block.settings.image.alt, 'img-about-2' }}
              {% else %}
                {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
              {% endif %}
            {% when 'text' %}
              <div class="medium-up--text-{{ block.settings.align_text }}">
                {% if block.settings.title != blank %}
                  <h4 class="h3-about2">{{ block.settings.title | escape }}</h4>
                {% endif %}
                {% if block.settings.text != blank %}
                  <div class="txt-about2 rte-setting rte">{{ block.settings.text }}</div>
                {% endif %}
              </div>
          {% endcase %}
        </div>
      </div>
    {% endfor %}
  </div>

  {% if section.blocks.size == 0 %}
    {% include 'no-blocks' %}
  {% endif %}
</div>

